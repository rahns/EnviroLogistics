{
  "name": "envirologistics",
  "author": {
    "name": "Rahn, Sam, Enrique & Hoang"
  },
  "contributors": [
    {
      "name": "Rahn Stavar"
    },
    {
      "name": "Sam"
    },
    {
      "name": "Enrique"
    },
    {
      "name": "Hoang"
    }
  ],
  "description": "A vehicle fleet route optimisation application, focused on reducing CO2 emissions.",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@mapbox/mapbox-gl-geocoder": "^4.7.3",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/pickers": "^3.3.10",
    "@react-firebase/auth": "^0.2.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "concurrently": "^6.2.1",
    "date-fns": "^2.23.0",
    "distinct-colors": "^3.0.0",
    "electron-is-dev": "^2.0.0",
    "express": "^4.17.1",
    "firebase": "^8.9.0",
    "mapbox": "^1.0.0-beta10",
    "mapbox-gl": "^2.4.1",
    "react": "^17.0.2",
    "react-awesome-modal": "^2.0.5",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-firebaseui": "^5.0.2",
    "react-scripts": "4.0.3",
    "react-simple-modal-provider": "^1.7.1",
    "reactjs-popup": "^2.0.5",
    "read-excel-file": "^5.2.8",
    "recharts": "^2.1.0",
    "wait-on": "^6.0.0",
    "web-vitals": "^1.1.2",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "electron": "^14.0.0",
    "electron-builder": "^22.11.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-native": "concurrently \"set BROWSER=none && npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "react-scripts build",
    "build-native": "electron-builder -c.extraMetadata.main='build/electron.js' --win",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-no-new-tab": "set BROWSER=none && react-scripts start",
    "prebuild-native": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.team17.envirologistics",
    "productName": "EnviroLogistics",
    "win": {
      "target": ["nsis"],
      "icon": "build/icon-512x512.png"
    },
    "mac": {
      "target": "dmg",
      "category": "public.app-category.business",
      "icon": "build/icon-512x512.png"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
